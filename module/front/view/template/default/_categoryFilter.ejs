<template id="categoryFilter">
  <div class="form-attr form-group row">
    <label class="col-xl-2 col-lg-3 col-form-label">{{label}}</label>
    <div class="col-xl-10 col-lg-9">
      <select v-model="value" class="form-select">
        <option value=""></option>
        <option v-for="item of items" :value="item.value">{{item.text}}</option>
      </select>
    </div>
  </div>
</template>